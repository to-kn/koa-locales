		"supertest": "^7.1.1",
		"vitest": "^3.2.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.6",
		"@types/debug": "^4.1.12",
		"@types/ini": "^4.1.1",
		"@types/js-yaml": "^4.0.9",
		"@types/koa": "^2.15.0",
		"@types/node": "^24.0.10",
		"@types/supertest": "^6.0.3",
